        android:theme="@style/AppTheme">
        <activity android:name="com.example.user.fcc.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.user.fcc.MenuPrincipal" />
        <activity android:name="com.example.user.fcc.Settings" />
